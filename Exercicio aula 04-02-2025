public class Carro {
   private int Km;
   private boolean alugado;


   public int getKm(){
       return Km;
   }


   public boolean estaAlugado() {
       return alugado;
   }


   public void setKm(int km) {
       Km = km;
   }


   public void setAlugado(boolean alugado) {
       this.alugado = alugado;
   }


   public void adicionarKm(int Km){
        this.Km = 20;
   }
}
